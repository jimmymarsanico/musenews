{
  "manifest_version": 2,

  "name": "Muse News",
  "description": "You know careers are a big deal, right? Well we’re here to make sure you’re super happy with yours.",
  "version": "3.0",

  "icons": {
    "16": "static/img/logo/CompanyMuseLogo_16.png",
  	"48": "static/img/logo/CompanyMuseLogo_48.png",
  	"64": "static/img/logo/CompanyMuseLogo_64.png",
  	"128": "static/img/logo/CompanyMuseLogo_128.png"
   },

  "chrome_url_overrides" : {
    "newtab": "app/dashboard.html"
  },

  "permissions": [
    "activeTab",
    "geolocation",
    "chrome://favicon/",
  	"tabs",
    "http://www.themuse.com/api/location",
    "https://www.themuse.com/api/location"
  ]
}
